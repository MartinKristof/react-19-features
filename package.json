{
  "name": "react-19-features",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "preview": "vite preview",
    "pretty:fix": "prettier --write .",
    "pretty": "prettier --check .",
    "qa": "yarn lint && yarn pretty",
    "backend": "json-server --watch db.json --port 3030 --middlewares fakeErrorMiddleware.cjs",
    "backend:delayed": "json-server --watch db.json --port 3030  --middlewares fakeErrorMiddleware.cjs --delay 1500",
    "prepare": "husky"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "framer-motion": "^12.11.0",
    "lucide-react": "^0.509.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.3",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/vite": "^4.1.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "json-server": "0.17.4",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-checker": "^0.9.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
